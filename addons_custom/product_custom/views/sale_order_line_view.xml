<odoo>
    <record id="view_order_form_inherit_invoiceable" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.invoiceable</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/list/field[@name='qty_delivered']" position="after">
                <field name="is_invoiceable" widget="is_invoiceable_icon"/>
            </xpath>
        </field>
    </record>
</odoo>