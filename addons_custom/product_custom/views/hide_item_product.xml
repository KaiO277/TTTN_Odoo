<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="product_template_form_view_hide_label_button" model="ir.ui.view">
            <field name="name">product.template.form.inherit.hide_label_button</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/> <!-- Sửa ID nếu cần -->
            <field name="arch" type="xml">

                <!--Ẩn button Print Labels trong form thêm sản phẩm-->
                <xpath expr="//button[@name='action_open_label_layout']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

            </field>
        </record>
    </data>
</odoo>